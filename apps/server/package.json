{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build --target=bun --outdir=dist ./src/server.ts",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/server.ts --outfile stl-shelf-server",
    "dev": "bun run --hot ./src/server.ts",
    "dev:docker": "docker-compose -f ../../../docker-compose.development.yml up -d && bun run dev",
    "ngrok": "./scripts/start-ngrok.sh",
    "start": "bun run ./dist/server.js",
    "deploy": "echo 'Use Docker for deployment'",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop"
  },
  "dependencies": {
    "@amandaghassaei/stl-parser": "1.7.0",
    "@orpc/client": "1.8.9",
    "@orpc/server": "1.8.9",
    "@polar-sh/better-auth": "^1.1.7",
    "@polar-sh/sdk": "^0.35.4",
    "@t3-oss/env-core": "0.13.8",
    "@types/bun": "1.2.22",
    "@types/multer": "2.0.0",
    "archiver": "^7.0.1",
    "better-auth": "1.3.9",
    "dotenv": "17.2.2",
    "drizzle-kit": "0.31.4",
    "drizzle-orm": "0.44.5",
    "hono": "4.10.3",
    "multer": "2.0.2",
    "nodemailer": "7.0.6",
    "postgres": "3.4.7",
    "three": "0.180.0",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@better-auth/cli": "1.3.9",
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.13.11",
    "@types/nodemailer": "^7.0.1",
    "tsdown": "0.15.1",
    "typescript": "^5.8.2",
    "wrangler": "^4.23.0"
  }
}
